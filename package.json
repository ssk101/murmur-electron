{
  "name": "murmur-electron",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "murmur",
    "mac": {
      "category": "hi"
    }
  },
  "browserify": {
    "transform": [
      [
        "fn-pug/lib/transform/browserify"
      ],
      "stylify",
      [
        "babelify",
        {
          "extensions": [
            ".js",
            ".pug"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@scoutgg/widgets": "^2.2.0",
    "moment": "^2.24.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-class-name": "github:zeddidragon/babel-plugin-class-name",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "construct-style-sheets-polyfill": "^2.2.10",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-fetch": "^1.4.0",
    "electron-packager": "^14.0.6",
    "electron-pug": "^2.0.0",
    "fn-pug": "github:scoutgg/fn-pug",
    "postcss-preset-env": "^6.7.0",
    "python-shell": "^1.0.8",
    "stylify": "^1.4.0",
    "stylus": "^0.54.7"
  }
}
